{
  "name": "js-utils",
  "version": "1.0.0",
  "description": "a vary awesome utils",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "./node_modules/mocha/bin/_mocha -- --ui bdd -R spec -t 5000 test/**/*.js",
    "doc": "jsdoc -c jsdoc.json",
    "doc:server": "npm run doc && http-server docs/ -p 9090 >/dev/null 2>&1 &",
    "commitmsg": "npm run lint && npm test && validate-commit-msg",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lodash": "^4.17.10",
    "require-directory": "^2.1.1"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "commitizen": "^2.9.6",
    "docdash": "^0.4.0",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.1.0",
    "http-server": "^0.11.1",
    "jsdoc": "^3.5.5",
    "mocha": "^5.1.1",
    "standard": "^11.0.1",
    "validate-commit-message": "^3.0.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
